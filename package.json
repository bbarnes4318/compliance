{
  "name": "medicare-aca-compliance-system",
  "version": "1.0.0",
  "description": "Medicare and ACA Live Transfer Call Center Compliance System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "build": "echo 'Build complete'",
    "migrate": "node src/database/migrate.js",
    "seed": "node src/database/seed.js",
    "screening:daily": "node -e \"require('./src/services/employeeScreening').performMonthlyScreening()\"",
    "audit:monthly": "node -e \"require('./src/services/documentRetention').generateRetentionReport()\"",
    "postinstall": "npm run migrate || echo 'Migration will run on first start'"
  },
